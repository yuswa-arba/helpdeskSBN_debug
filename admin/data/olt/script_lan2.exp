#!/usr/bin/expect -f
spawn telnet 172.17.8.35
set timeout 3
expect "Username:"
send "admin\r"
expect "Password:"
send "globalbdcom\r"
expect ">"
send "ena\r"
expect "password:"
send "globalbdcom\r"
expect "#"
send "show epon interface ePON 0/1:7 onu port 2 state\r"
expect "#"
set i 1
while {$i < 51} {
set output [open "/usr/script/txt/aditamalan-2-status.txt" "w"]
set outcome $expect_out(buffer)
puts $output $outcome
incr i
}
close $output
expect "#"
send "exit\r"